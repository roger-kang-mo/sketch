<div id="login" class="col-md-4 col-md-offset-4 pagination-centered">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-group">
      <input type="email" class="form-control" placeholder="Email" id="user_email" name="user[email">
    </div>
    <div class="form-group">
      <input type="password" autocomplete="off" class="form-control"  placeholder="password" id="user_password" name="user[password]">
    </div>
    <div class="form-group">
      <%= f.check_box :remember_me %> <%= f.label :remember_me %>
    </div>
    <!-- <div class="form-group">
      <button id="submit" class="form-control">Go</button>
    </div> -->

    <div class="form-group">
      <input name="commit" class="form-control" type="submit" value="Go">
    </div>
  <% end %>

  <%= render "devise/shared/links" %>
</div>

